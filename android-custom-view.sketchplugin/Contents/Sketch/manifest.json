{
    "name": "AndroidVectoView",
    "description" : "A plugin which create custom view from sketch",
    "author": "Alexey Petrushin",
    "version" : "1.1",
    "authorEmail" : "petrushinalex@gmail.com",
    "identifier": "com.petrushin.alex.android.vector.view",

    "compatibleVersion": 3,
    "bundleVersion": 1,
    "commands":
        [
            {
                "name": "Generate custom view",
                "identifier": "path-view",
                "shortcut": "shift t",
                "script": "generate_view.js",
                "handler": "onRun"
            }
        ],
    "menu": {
        "items": [
            "path-view"
        ],
        "isroot": "true"
    }
}
